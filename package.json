{
  "name": "react-mixitup",
  "version": "3.0.0",
  "description": "Animate the addition, removal and shuffling of elements.",
  "keywords": [
    "react",
    "mixitup",
    "shuffle",
    "grid",
    "animate",
    "transition"
  ],
  "main": "dist/react-mixitup.umd.js",
  "module": "dist/react-mixitup.es5.js",
  "typings": "dist/types/react-mixitup.d.ts",
  "files": [
    "dist"
  ],
  "author": "Richie<oss@ricsam.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ricsam/react-mixitup.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "test": "react-scripts test --env=jsdom"
  },
  "resolutions": {
    "@types/eslint": "8.4.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts,tsx}"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "prettier": "^1.14.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.2",
    "typedoc": "^0.22.12",
    "typescript": "~4.4.2",
    "web-vitals": "^2.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8"
  }
}
